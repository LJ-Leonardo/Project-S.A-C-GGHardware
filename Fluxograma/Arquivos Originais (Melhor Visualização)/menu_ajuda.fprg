    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;MENU DE AJUDA&quot;" newline="True"/>
            <output expression="&quot;1. Suporte com Produtos&#13;&#10;2. Suporte com Clientes&quot;" newline="True"/>
            <output expression="&quot;3. Suporte com Fornecedores&#13;&#10;4. Suporte com Parceiros&quot;" newline="True"/>
            <output expression="&quot;5. Insatisfa&#231;&#227;o / D&#250;vidas n&#227;o supridas&#13;&#10;6. Suporte com Vendas&quot;" newline="True"/>
            <output expression="&quot;7. Suporte com Estoque&#13;&#10;8. Retornar ao Menu Principal&quot;" newline="True"/>
            <if expression="&quot;Escolha:&quot;">
                <then>
                    <if expression="&quot;Insatisfa&#231;&#227;o &#224; Retornar ao Menu Principal&quot;">
                        <then>
                            <if expression="&quot;Estoque e Retornar ao Menu Principal&quot;">
                                <then>
                                    <call expression="RETORNAR"/>
                                    <output expression="&quot;Retornar ao Menu Principal&quot;" newline="True"/>
                                </then>
                                <else>
                                    <call expression="ESTOQUE"/>
                                    <output expression="&quot;Informa&#231;&#245;es e Instru&#231;&#245;es sobre como acessar a aba Estoque&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <if expression="&quot;Insatisfa&#231;&#227;o ou Venda&quot;">
                                <then>
                                    <call expression="VENDA"/>
                                    <output expression="&quot;Como efetivar uma venda, lista de produtos dispon&#237;veis, como cancelar vendas e hist&#243;rico de vendas&quot;" newline="True"/>
                                </then>
                                <else>
                                    <call expression="INSATISFA&#199;&#195;O"/>
